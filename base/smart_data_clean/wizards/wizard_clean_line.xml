<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="wizard_clean_form" model="ir.ui.view">
            <field name="name">wizard.clean.from</field>
            <field name="model">wizard.clean</field>
            <field name="arch" type="xml">
                <form string="">
                    <h2>Add those lines to Data Clean Setting</h2>
                    <group>
                        <field name="line_ids" nolabel="1">
                            <tree editable="top" create="false">
                                <field name="model_id"/>
                                <field name="model"/>
                            </tree>
                        </field>
                    </group>
                    <footer>
                        <button name="apply" string="Confirm" type="object" class="btn-primary"/>
                        <button special="cancel" string="Cancel" class="btn-secondary"/>
                    </footer>
                </form>
            </field>

        </record>



    </data>
</odoo>
